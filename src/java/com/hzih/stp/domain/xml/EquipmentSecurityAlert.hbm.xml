<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="equipment_security_alert" name="com.hzih.stp.domain.EquipmentSecurityAlert">
    <id access="field" name="id" type="java.lang.Long" column="id">
      <generator class="increment"/>
    </id>
    <property name="equipmentName" column="equipment_name" access="field" type="java.lang.String"/>
    <property name="alertTime" column="alert_time" access="field" type="java.util.Date"/>
    <property name="alertInfo" column="alert_info" access="field" type="java.lang.String"/>
    <property name="ip" column="ip" access="field" type="java.lang.String"/>
    <property name="isRead" column="isread" access="field" type="java.lang.String"/>
  </class>
</hibernate-mapping>
